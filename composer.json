{
  "name": "evilstudio/composer-parser",
  "type": "project",
  "description": "Simple app for parse composer.json file from multiple repositories to one Excel spreadsheet",
  "keywords": ["composer", "parser", "git", "spreadsheet", "Excel", "xlsx", "xls"],
  "license": "MIT",
  "authors": [
    {
      "name": "Dariusz Matoga",
      "email": "dmatoga@evilstudio.pl"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "ext-zip": "*",
    "czproject/git-php": "^4.0",
    "daniel-ness/ansible-vault": "^0.0.3",
    "mikehaertl/php-shellcommand": "^1.6",
    "php-curl-class/php-curl-class": "^9.5",
    "phpoffice/phpspreadsheet": "^1.19",
    "symfony/config": "^6.3",
    "symfony/console": "^6.3",
    "symfony/dependency-injection": "^6.3",
    "symfony/expression-language": "^6.3",
    "symfony/filesystem": "^6.3",
    "symfony/yaml": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "EvilStudio\\ComposerParser\\": "src/"
    }
  }
}
